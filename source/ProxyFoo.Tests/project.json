{
  "version": "1.0.0-*",
  "description": "ProxyFoo.Tests Class Library",
  "authors": [ "Jamie da Silva" ],
  "buildOptions": { "define": [ "NUNIT3" ] },
  "frameworks": {
    ".NETFramework,Version=v4.0,Profile=Client": {
      "buildOptions": { "define": [ "NET40", "FEATURE_LEGACYREFLECTION", "FEATURE_SAVEASSEMBLY" ] }
    },
    "net45": {
      "buildOptions": { "define": [ "NET40", "NET45", "FEATURE_SAVEASSEMBLY" ] },
      "frameworkAssemblies": {
        "Microsoft.Build.Utilities.v4.0": "4.0.0.0"
      }
    },
    "netstandard1.5": {
      "imports": ["dotnet"],
      "buildOptions": { "define": [ "NETSTANDARD" ] },
      "dependencies": {
        "Microsoft.CSharp": "4.0.1-rc2-24027",
        "System.Collections": "4.0.11-rc2-24027",
        "System.IO": "4.1.0-rc2-24027",
        "System.Linq": "4.1.0-rc2-24027",
        "System.Runtime": "4.1.0-rc2-24027",
        "System.Threading": "4.0.11-rc2-24027",
        "System.Runtime.Extensions": "4.1.0-rc2-24027",
        "System.Reflection.TypeExtensions": "4.1.0-rc2-24027"
      }
    }
  },
  "dependencies": {
    "NUnit": "3.0.1",
    "ProxyFoo": {
      "target": "project"
    }
  }
