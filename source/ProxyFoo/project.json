{
  "version": "0.4.0-*",
  "description": "ProxyFoo is a library for the .NET Framework to facilitate creating high-performance proxies for Duck casting, null safe wrappers, method interception, and other uses.",
  "authors": [ "Silverlake Software LLC" ],
  "packOptions": {
    "owners": [ "Jamie da Silva" ],
    "tags": [ "ProxyFoo Duck DuckTyping Dynamic Proxy Typing Casting Intercept Mixin" ],
    "projectUrl": "https://github.com/ProxyFoo/ProxyFoo",
    "licenseUrl": "http://www.apache.org/licenses/LICENSE-2.0.html",
    "requireLicenseAcceptance": false
  },
  "copyright": "Copyright © 2012-2016 Silverlake Software LLC",

  "frameworks": {
    "net40-client": {
      "buildOptions": { "define": [ "NET40", "FEATURE_LEGACYREFLECTION", "FEATURE_SAVEASSEMBLY" ] }
    },
    "net45": {
      "buildOptions": { "define": [ "NET40", "NET45", "FEATURE_SAVEASSEMBLY" ] },
      "frameworkAssemblies": {
        "System.Reflection": "4.0.0.0"
      }
    },
    "netstandard1.5": {
      "buildOptions": { "define": [ "NETSTANDARD", "FEATURE_NOAPPDOMAIN" ] },
      "dependencies": {
        "System.Linq": "4.1.0",
        "System.Threading": "4.0.11",
        "System.Reflection": "4.1.0",
        "System.Reflection.Extensions": "4.0.1",
        "System.Reflection.Emit": "4.0.1",
        "System.Reflection.Emit.ILGeneration": "4.0.1",
        "System.Reflection.TypeExtensions":  "4.1.0",
        "System.Diagnostics.Debug": "4.0.11",
        "System.Collections.Concurrent": "4.0.12"
      }
    }
  }
}
